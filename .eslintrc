{
  "root": true,
  "parser": "babel-eslint",
  "parserOptions": {
    "ecmaVersion": "2017",
    "sourceType": "module",
    "ecmaFeatures": {
      "jsx": true
    }
  },
  "env": {
    "node": true,
    "es6": true
  },
  "rules": {
    "array-bracket-spacing": 1,
    "arrow-parens": 2,
    "arrow-spacing": 1,
    "block-spacing": 1,
    "brace-style": [1, "1tbs", {
      "allowSingleLine": true
    }],
    "camelcase": 1,
    "comma-dangle": [2, "always-multiline"],
    "comma-spacing": [1, {
      "before": false,
      "after": true
    }],
    "comma-style": [1, "last"],
    "consistent-this": [2, "self"],
    "constructor-super": 2,
    "curly": 2,
    "default-case": 2,
    "dot-notation": 1,
    "eol-last": 1,
    "eqeqeq": [2, "always"],
    "func-call-spacing": [1, "never"],
    "func-names": [1, "always"],
    "func-style": [1, "expression"],
    "generator-star-spacing": [1, {
      "before": false,
      "after": true
    }],
    "id-length": 2,
    "indent": [1, 2, {
      SwitchCase: 1
    }],
    "jsx-quotes": [1, "prefer-single"],
    "key-spacing": [1, {
      "mode": "strict"
    }],
    "keyword-spacing": [1, {
      "before": true,
      "after": true
    }],
    "max-len": [1, 100],
    "new-cap": [1, {
      "newIsCap": true,
      "capIsNew": true,
      "properties": true
    }],
    "new-parens": 2,
    "newline-per-chained-call": [1, {
      "ignoreChainWithDepth": 2
    }],
    "no-array-constructor": 2,
    "no-case-declarations": 2,
    "no-class-assign": 2,
    "no-cond-assign": [2, "except-parens"],
    "no-confusing-arrow": [2, {
      "allowParens": true
    }],
    "no-console": 1,
    "no-const-assign": 2,
    "no-constant-condition": 2,
    "no-control-regex": 2,
    "no-debugger": 2,
    "no-delete-var": 2,
    "no-dupe-args": 2,
    "no-dupe-class-members": 2,
    "no-dupe-keys": 2,
    "no-duplicate-case": 2,
    "no-duplicate-imports": 1,
    "no-else-return": 1,
    "no-empty": 2,
    "no-empty-character-class": 2,
    "no-empty-function": 2,
    "no-empty-pattern": 2,
    "no-extra-bind": 2,
    "no-extra-label": 1,
    "no-eval": 2,
    "no-ex-assign": 2,
    "no-extra-boolean-cast": 2,
    "no-extra-parens": 1,
    "no-fallthrough": 2,
    "no-func-assign": 2,
    "no-global-assign": 2,
    "no-inner-declarations": 2,
    "no-invalid-regexp": 2,
    "no-irregular-whitespace": 2,
    "no-lonely-if": 1,
    "no-loop-func": 2,
    "no-mixed-operators": 1,
    "no-mixed-spaces-and-tabs": [2, "smart-tabs"],
    "no-multi-spaces": 1,
    "no-multiple-empty-lines": [1, {
      "max": 2
    }],
    "no-new-func": 2,
    "no-new-object": 1,
    "no-new-symbol": 2,
    "no-obj-calls": 2,
    "no-octal": 2,
    "no-param-reassign": [2, {
      "props": true
    }],
    "no-proto": 2,
    "no-redeclare": 2,
    "no-regex-spaces": 2,
    "no-return-assign": 1,
    "no-self-assign": 2,
    "no-sparse-arrays": 2,
    "no-tabs": 1,
    "no-this-before-super": 2,
    "no-trailing-spaces": 1,
    "no-unexpected-multiline": 2,
    "no-unneeded-ternary": 1,
    "no-undef": 2,
    "no-underscore-dangle": 1,
    "no-unreachable": 2,
    "no-unsafe-finally": 2,
    "no-unused-labels": 2,
    "no-unused-vars": 2,
    "no-use-before-define": [2, {
      "functions": false,
      "classes": false
    }],
    "no-useless-constructor": 1,
    "no-useless-escape": 1,
    "no-whitespace-before-property": 1,
    "no-var": 1,
    "object-curly-spacing": [1, "always", {
      "arraysInObjects": true,
      "objectsInObjects": true
    }],
    "object-property-newline": 1,
    "object-shorthand": [1, "always"],
    "operator-linebreak": [1, "before"],
    "padded-blocks": [1, "never"],
    "prefer-arrow-callback": 1,
    "prefer-const": 1,
    "prefer-rest-params": 1,
    "prefer-spread": 1,
    "prefer-template": 1,
    "quote-props": [1, "as-needed"],
    "quotes": [1, "single"],
    "radix": [2, "always"],
    "require-yield": 2,
    "rest-spread-spacing": [1, "never"],
    "semi": [2, "always"],
    "space-before-blocks": 1,
    "space-in-parens": [1, "never"],
    "space-infix-ops": [1, {
      "int32Hint": false
    }],
    "spaced-comment": [1, "always"],
    "template-curly-spacing": 1,
    "use-isnan": 2,
    "valid-typeof": 2,
    "wrap-iife": [1, "outside"],
    "yield-star-spacing": [1, "after"]
  }
}
